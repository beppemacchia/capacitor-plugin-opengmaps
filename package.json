{
  "name": "capacitor-plugin-opengmaps",
  "version": "0.0.5",
  "description": "Capacitor OpenGmaps Plugin",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Giuseppe Macchia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beppemacchia/capacitor-plugin-opengmaps.git"
  },
  "bugs": {
    "url": "https://github.com/beppemacchia/capacitor-plugin-opengmaps/issues"
  },
  "devDependencies": {
    "@capacitor/android": "^2.4.6",
    "@capacitor/core": "^2.4.6",
    "@capacitor/ios": "^2.4.6",
    "@ionic/prettier-config": "^1.0.1",
    "@ionic/swiftlint-config": "^1.1.2",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-java": "^1.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.1",
    "swiftlint": "^1.0.1",
    "typescript": "~3.8.3"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.6"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorPluginOpengmaps.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config"
}
